#include <string>
#include <vector>

using namespace std;

string solution(string s) {
    string answer = "";
    int len=s.size();
    if(s.size()%2==0){
        return s.substr(len/2-1,2);
    }
    else{
        return s.substr(len/2,1);
    }
}
