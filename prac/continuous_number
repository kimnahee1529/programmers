//https://programmers.co.kr/learn/courses/30/lessons/12906?language=cpp
#include <vector>
#include <iostream>

using namespace std;

vector<int> solution(vector<int> arr) {
    vector<int> answer;
    char c=arr[0];
    answer.push_back(arr.at(0));
    for(int i=1; i<arr.size(); i++){
        if(c!=arr.at(i)){
            answer.push_back(arr.at(i));
            c=arr.at(i);
        }
    }
    return answer;
}
