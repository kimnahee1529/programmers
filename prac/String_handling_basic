//https://programmers.co.kr/learn/courses/30/lessons/12918?language=cpp
#include <string>

using namespace std;

bool solution(string s) {
    if(s.size()==4 || s.size()==6){
        for(int i=0; i<s.size(); i++){
            if (!isdigit(s[i]))//47,58
                return false;
            }
        return true;
    }else
        return false;
}
//숫자인 판별하는 isdigit()
